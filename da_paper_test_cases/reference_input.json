{
  "ensemble":
  {
    "ensemble_simulation": true,
    "ensemble_size": 6,
    "materials":
    {
      "material_0":
      {
        "initial_temperature_stddev": 5.0,
        "new_material_temperature_stddev": 5.0,
        "solid":
        {
          "density_stddev": 1.0,
          "specific_heat_stddev": 1.0,
          "thermal_conductivity_x_stddev": 1.0,
          "thermal_conductivity_y_stddev": 1.0,
          "thermal_conductivity_z_stddev": 1.0,
          "convection_heat_transfer_coef_stddev": 0.02,
          "emissivity_stddev": 0.005
        },
        "liquid":
        {
          "specific_heat_sttdev": 1.0,
          "density_stddev": 1.0,
          "thermal_conductivity_x_stddev": 1.0,
          "thermal_conductivity_y_stddev": 1.0,
          "thermal_conductivity_z_stddev": 1.0,
          "convection_heat_transfer_coef_stddev": 0.02,
          "emissivity_stddev": 0.005
        },
        "solidus_stddev": 1.0,
        "liquidus_stddev": 1.0,
        "latent_heat_stddev": 100.0,
        "radiation_temperature_infty_stddev": 5.0,
        "convection_temperature_infty_stddev": 5.0
      }
    },
    "sources":
    {
      "beam_0":
      {
        "depth_stddev": 1e-03,
        "diameter_stddev": 1e-03,
        "absorption_efficiency_stddev": 0.02,
        "max_power_stddev": 0.05
      }
    }
  },

  "experiment":
  {
    "read_in_experimental_data": true,
    "log_filename": "observations/build_time_log.txt",
    "file": "observations/build_#camera_#frame.csv",
    "format": "ray",
    "first_frame": 0,
    "last_frame": 1000000000,
    "first_camera_id": 0,
    "last_camera_id": 0,
    "first_frame_temporal_offset": 0.0,
    "estimated_uncertainty": 5.0,
    "output_experiment_on_mesh": true
  },

  "data_assimilation":
  {
    "assimilate_data": true,
    "localization_cutoff_function": "gaspari_cohn",
    "localization_cutoff_distance": 5.0e-3,
    "solver":
    {
      "max_number_of_temp_vectors": 100,
      "convergence_tolerance": 1.0e-4,
      "max_iterations": 100000
    }
  },


  "units":
  {
    "mesh": "millimeter"
  },


  "geometry":
  {
    "import_mesh": true,
    "mesh_format": "abaqus",
    "mesh_file": "mesh.inp",
    "dim": 3,
    "reset_material_id": true,
    "material_deposition": true,
    "material_deposition_method": "scan_paths",
    "deposition_length": 5.0e-3,
    "deposition_height": 3.0e-3,
    "deposition_width": 10.0e-3,
    "deposition_lead_time": 1,
    "deposition_time": 10.0,
    "material_height": 0.0
  },

  "physics":
  {
    "thermal": true,
    "mechanical": false
  },

  "boundary":
  {
    "type": "convective"
  },


  "refinement":
  {
    "n_refinements": 0,
    "time_steps_between_refinement": 50000
  },

  "materials":
  {
    "n_materials": 1,
    "property_format": "polynomial",
    "material_0":
    {
      "initial_temperature": 300,
      "new_material_temperature": 300,
      "solid":
      {
        "density": 7955,
        "specific_heat": 650,
        "thermal_conductivity_x": 19.0,
        "thermal_conductivity_y": 19.0,
        "thermal_conductivity_z": 19.0,
        "convection_heat_transfer_coef": 0.12,
        "emissivity": 0.05
      },
      "liquid":
      {
        "specific_heat": 770.4,
        "density": 7955,
        "thermal_conductivity_x": 19.0,
        "thermal_conductivity_y": 19.0,
        "thermal_conductivity_z": 19.0,
        "convection_heat_transfer_coef": 0.1,
        "emissivity": 0.05
      },
      "solidus": 1670,
      "liquidus": 1730,
      "latent_heat": 2.68e5,
      "radiation_temperature_infty": 300,
      "convection_temperature_infty": 300
    }
  },

  "sources":
  {
    "n_beams": 1,
    "beam_0":
    {
      "type": "goldak",
      "depth": 13.3e-3,
      "diameter": 15.0e-3,
      "scan_path_file": "scan_path.inp",
      "scan_path_file_format": "event_series",
      "absorption_efficiency": 0.9,
      "max_power": 1.5
    }
  },

  "time_stepping":
  {
    "method": "forward_euler",
    "scan_path_for_duration": true,
    "time_step": 5.0e-3
  },

  "post_processor":
  {
    "filename_prefix": "solution",
    "time_steps_between_output": 200,
    "additional_output_refinement": 0
  },

  "discretization":
  {
    "thermal":
    {
      "fe_degree": 1,
      "quadrature": "gauss"
    },
    "mechanical":
    {
      "fe_degree": 1,
      "quadrature": "gauss:"
    }
  },


  "profiling":
  {
    "timer": false,
    "caliper": "loop-report,runtime-report(calc.inclusive,region.count,profile.kokkos)"
  },
  
  "memory_space": "host",
  "verbose_output": true
}
